			},
			Four_spoke_MFSW_for_radio_and_small_leather_pack {
				ifin: or(Fabia_Hatch_GreenLine_II);
			},
			Four_spoke_MFSW_for_telephone_and_radio_and_small_leather_pack {
				ifin: or(Fabia_Hatch_GreenLine_II);
			},
			Four_spoke_multifunctional_steering_wheel_with_radio_and_phone_controls_small_leather_package {
				ifin: or(Superb_Estate_S, Superb_Estate_SE);
			},
			Four_spoke_multifunctional_steering_wheel_with_radio_controls_small_leather_package {
				ifin: or(Superb_Estate_S);
				ifin: Maxi_dot_trip_computer;
			}
		} // other constraints
			!Superb_Estate_Elegance -> (
				Telephone_preparation && (Three_spoke_MFSW_for_radio_and_telephone_and_small_leather_pack ||
                   Three_spoke_multifunctional_leather_steering_wheel_with_radio_phone_and_DSG ||
				Three_spoke_multifunctional_leather_steering_wheel_with_control_module_for_radio_and_telephone ||
				Four_spoke_MFSW_for_telephone_and_radio_and_small_leather_pack ||
				Four_spoke_multifunctional_steering_wheel_with_radio_and_phone_controls_small_leather_package
			));
		},
		Floor_mats {
			ifin: or(Fabia_Hatch_vRS);
		},
		Front_armrest {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
		},
		Heated_seats group [1..1] {
			Heated_front_seats {
				ifin: or(Fabia_Hatch,
					Superb_Estate && ! Superb_Estate_Elegance);
			},
			Heated_front_and_rear_seats {
				ifin: or(Superb_Estate);
			}
		},
		Manually_adjustable_lumbar_supports {
			ifin: or(Superb_Estate_S);
		},
		Textile_floor_mats {
			ifin: or(Fabia_Hatch_S, Fabia_Hatch_GreenLine_II, Fabia_Hatch_SE, Fabia_Hatch_Monte_Carlo);
		},
		Third_rear_headrest {
			ifin: or(Fabia_Hatch, Yeti_E, Yeti_S);
		},
		Wood_decor {
			ifin: or(Superb_Estate_SE, Superb_Estate_SE_Plus, Superb_Estate_Elegance);
		}
