	Opt50_Wheel_tire group [0..1] {
		Wheel_15inch_Antares_alloys_6Jx15_195_55_R15_85H {
			ifin: or(Fabia_Hatch_S);
		},
		Wheel_16inch_Atria_alloys_6_5Jx16_205_45_R16_83W {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus);
		},
		Wheel_16inch_Elba_alloy_wheels {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
		},
		Wheel_17inch_Laurel_alloys_7_5Jx17_225_45_17_94W_Y {
