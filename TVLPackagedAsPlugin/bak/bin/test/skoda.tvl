		},
		Gigaro_Black_alloy_wheels_7Jx17_205_40_R17_7Jx17_ET41 {
			ifin: or(Fabia_Hatch_vRS);
		},
		Gigaro_Dark_Chrome_alloy_wheels_7Jx17_205_40_R17_7Jx17_ET41 {
			ifin: or(Fabia_Hatch_vRS);
		},
		Gigaro_White_alloy_wheels_7Jx17_205_40_R17_7Jx17_ET41 {
			ifin: or(Fabia_Hatch_vRS);
