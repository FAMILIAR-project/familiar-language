// Colors

Color group allof {
	// XXX list individual colors
	ColorExt group oneof {
		Metallic_colors,
		Metallic_colors_for_silver_roof {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
		},
		Metallic_colors_for_black_roof {
			ifin: or(Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
		},
		Metallic_colors_for_white_roof {
			ifin: or(Fabia_Hatch_vRS);
		},
		Solid_colors {
			ifin: ! or(Fabia_Hatch_SE_Plus, Fabia_Hatch_Monte_Carlo);
		},
		Solid_colors_for_silver_roof {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
		},
		Solid_colors_for_black_roof {
			ifin: or(Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
		},
		Solid_colors_for_white_roof {
			ifin: or(Fabia_Hatch_vRS);
		},
		Special_colors {
			ifin: or(Fabia_Hatch && !Fabia_Hatch_Monte_Carlo);
		}
	},
	ColorInt group oneof {
		ColorInt_normal,
		ColorInt_special_SE {
			ifin: or(Fabia_Hatch_SE, Superb_Estate_SE);
		},
		ColorInt_special_L_K {
			ifin: or(Superb_Estate_Elegance);
		},
		ColorInt_MonteCarlo_red {
			ifin: or(Fabia_Hatch_Monte_Carlo);
		}
	}
}

// Options

Option group [0..*] {
	Opt10_Safety group [0..*] {
		Curtain_airbags {
			ifin: or(Fabia_Hatch && ! Fabia_Hatch_vRS);
		},
		Rear_side_airbags {
			ifin: or(Superb_Estate);
		},
		Stability group [1..1] {
			ABS_plus_TPM {
				ifin: or(Fabia_Hatch_SE);
				ifin: ! TPM;
			},
			ESP_plus_ASR {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo);
			},
			ESP_TPM_HHC {
				ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo);
				ifin: ! TPM;
			},
			HHC_TPM {
				ifin: or(Superb_Estate_S, Superb_Estate_SE_Plus, Superb_Estate_Elegance);
				ifin: ! TPM;
			},
			ESP_ABS_EBV_MSR_ASR_EDS_MBA_DSR {
				ifin: or(Yeti_E, Yeti_S);
			}
		},
		Bi_xenon_headlight_with_AFS {
			ifin: or(Superb_Estate_SE, Superb_Estate_SE_Plus);
			ifin: ! Headlight_washers;
		},
		Fog_lights group [1..1] {
			Cornering_front_fog_lights {
				ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
			},
			Front_fog_lights {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus);
			},
			Curve_projector_halogen_headlights_including_cornering_front_fog_lights {
				ifin: or(Fabia_Hatch_Elegance);
			}
		},
		TPM {
			ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo,
				Superb_Estate_S, Superb_Estate_SE_Plus);
		}
	},
	Opt20_Functionality { group [0..*] {
		Glovebox_cover {
			ifin: or(Fabia_Hatch_S);
		},
		Glasses_storage_compartment {
			ifin: or(Fabia_Hatch_vRS);
		},
		Sports_suspension {
			ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
		},
		Storage_box group [1..1] {
			Under_passenger_seat {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
			},
			Under_driver_seat {
				ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
			},
			Front_Seats {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo, Fabia_Hatch_vRS);
			}
		},
		Tow_bar group [1..1] {
			Preparation_for_tow_bar {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance,
					Superb_Estate, Yeti_S);
			},
			Tow_bar_removable {
				ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance);
			}
		},
		Roof_rails_eloxed {
			ifin: or(Superb_Estate_S);
		},
		Silver_roof_rails {
			ifin: or(Yeti_S);
		},
		Electrically_operated_tailgate {
			ifin: or(Superb_Estate);
		},
		Heated_windshield_washer_nozzles {
			ifin: or(Superb_Estate);
		},
		Net_programme {
			ifin: or(Superb_Estate, Yeti_S && Variable_boot_floor);
		},
		Retractable_parcel_shelf {
			ifin: or(Superb_Estate);
		},
		Mechanical_roller_sun_blinds_for_rear_side_windows {
			ifin: or(Superb_Estate);
		},
		Smoking_package {
			ifin: or(Superb_Estate);
		},
		Floor_mats group [1..1] {
			Textile_floor_mats {
				ifin: or(Superb_Estate);
			},
			L_K_front_and_rear_floor_mats {
				// already constrained
			}
		},
		Boot_floor group [1..1] {
			Boot_floor_with_integrated_aluminium_strips_and_fastening_elements {
				ifin: or(Superb_Estate_S);
			},
			Variable_boot_floor {
				ifin: or(Superb_Estate, Yeti_S && Net_programme);
			}
		},
		Rough_road_package {
			ifin: or(Yeti_S);
		},
		Textile_floor_mats {
			ifin: or(Yeti_S);
		}
	} // other constraints
		Electrically_operated_tailgate excludes Retractable_parcel_shelf;
	},
	Opt30_Interior_functionality group [0..*] {
		Steering_wheel { group [1..1] {
			Three_spoke_leather_steering_wheel_and_small_leather_pack {
				ifin: or(Fabia_Hatch_SE, Fabia_Hatch_Elegance);
			},
			Three_spoke_MFSW_for_radio_and_small_leather_pack {
				ifin: or(Fabia_Hatch_SE, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo);
			},
			Three_spoke_MFSW_for_radio_and_telephone_and_small_leather_pack {
