	M_16_TDI_CR_90PS_5spd_DPF { ifin: or(Fabia_Hatch_S, Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Estate_S, Fabia_Estate_SE, Fabia_Estate_SE_Plus, Fabia_Estate_Elegance, Fabia_Estate_Scout); },
	M_16_TDI_CR_105PS_5spd_DPF { ifin: or(Fabia_Hatch_SE, Fabia_Hatch_SE_Plus, Fabia_Hatch_Elegance, Fabia_Hatch_Monte_Carlo, Fabia_Estate_SE, Fabia_Estate_SE_Plus, Fabia_Estate_Elegance, Fabia_Estate_Scout); },

