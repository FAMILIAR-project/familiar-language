	M_20_TDI_CR_140PS_4x4_DSG_DPF { ifin: or(Yeti_SE, Yeti_SE_Plus, Yeti_Elegance); },
	M_20_TDI_CR_170PS_DPF { ifin: or(Octavia_Hatch_vRS); },
	M_20_TDI_CR_170PS_DSG_DPF { ifin: or(Octavia_Hatch_vRS); },

