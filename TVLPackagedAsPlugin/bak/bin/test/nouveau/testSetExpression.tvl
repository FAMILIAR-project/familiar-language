enum color in { Red, Blue, White };
int weightSet in { 800, 900, 1000 };
struct size {
	int length in {4, 5, 6 };
	weightSet weight;
	int height;
}	
root Car {
	size mainSize;
	size secondSize {
		length;
		weight;
		height in { 120, 130, 140 };
	}
	color carColor;	
	group oneof {
		Ferrari, 
		Ford
	}
}