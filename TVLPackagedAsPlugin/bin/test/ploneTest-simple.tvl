root PloneMeeting {
    IsDefault requires Tags;

        group [3..3] {
            General {
                  group [0..1] {
                      IsDefault 
                  }
            },
            DonneesA {
                  group [0..5]{
                       AssociatedGroups,
                       Tags,
					   Keywords,
					   Observations,
					   ToDiscuss
                  }
            },
            DonneesB {
                group [1..2] {
				   UseGroupsAsCategories,
				   DefaultToDiscussNormalItems,
				   DefaultToDiscussLateItems
				}
            }
       }
	
}