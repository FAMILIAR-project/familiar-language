const int maxCarSpeed 240;
const int maxTruckSpeed 130;
const real maxCarPrice 7500.0;
const real maxTruckPrice 17500.0;
const bool usedCar true;
const bool usedTruck false;
root Vehicle {
	int speed is (Car ? maxCarSpeed : maxTruckSpeed);
	bool used is (Car ? ! usedCar : usedTruck);
	real price is (Car ? maxCarPrice : maxTruckPrice);
	group oneof {
		Car {
			int speed is maxCarSpeed;
			bool used is ! usedCar;
			int price is 6000;
		},
		Truck {
			int speed is maxTruckSpeed;
			bool used is usedTruck;
			int price is 15000;
		}
	}
}