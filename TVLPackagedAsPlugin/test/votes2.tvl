root Votes {


AvailableVoteValues <->  EnableVoting;
	DefaultVoteValue.YesDVV requires AvailableVoteValues.YesAVV;
	DefaultVoteValue.NoDVV requires AvailableVoteValues.NoAVV;
	DefaultVoteValue.AbstentionDVV requires AvailableVoteValues.AbstentionAVV;
	group [3..3] {
		Artificial1 {
			group [0..1] {
				EnableVoting	
			}
		},
		Artificial2 {
			group [0..1] {
				AvailableVoteValues {
					group [1..3] {
						YesAVV,
						NoAVV,
						AbstentionAVV
					}	
				}	
			}
		},
		Artificial3 {
			group [0..1] {
				DefaultVoteValue {
					group [1..1] {
						YesDVV,
						NoDVV,
						AbstentionDVV
					}	
				}	
			}
		}
	}
}
